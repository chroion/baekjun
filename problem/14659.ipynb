   "source": [
    "n=int(input())\n",
    "archers=list(map(int,input().split()))\n",
    "mem=[0]*n\n",
    "i=0\n",
    "j=1\n",
    "while i<n-1: \n",
    "    if i+j<n and archers[i]>archers[i+j]: \n",
    "        mem[i]=j\n",
    "        j+=1\n",
    "    else:\n",
    "        i=max(i,i+j)\n",
    "        j=1\n",
    "print(max(mem))"
   ]
  }
