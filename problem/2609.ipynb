    "m,n=(map(int,input().split()))\n",
    "m,n=max(m,n),min(m,n)\n",
    "\n",
    "def euclidean(a, b):\n",
    "    c = a % b\n",
    "    if c == 0:\n",
    "        return b\n",
    "    else:\n",
    "        return euclidean(b, c)\n",
    "    \n",
    "gcd=euclidean(m,n)\n",
    "lcm=m*n/gcd\n",
    "\n",
    "print(gcd)\n",
    "print(int(lcm))"
   ]
  }
