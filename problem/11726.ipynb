    "n=int(input())\n",
    "mem=[0]*(n+1)\n",
    "\n",
    "if n>=3:\n",
    "    mem[1]=1\n",
    "    mem[2]=2\n",
    "    for i in range(3,n+1):\n",
    "        mem[i]=mem[i-1]+mem[i-2]\n",
    "    print(mem[n]%10007)\n",
    "elif n==1:\n",
    "    print(1)\n",
    "else:\n",
    "    print(2)"
   ]
